#!/bin/bash

printUsage()
{
    cat <<HELPTEXT
Print the user/repository part of a GitHub repository.
HELPTEXT
    echo
    printf 'Usage: %q %s\n' "$(basename "$1")" '[-?|-h|--help]'
}
case "$1" in
    --help|-h|-\?)	shift; printUsage "$0"; exit 0;;
esac

url="$(hub-url "$@")" || exit $?
[[ "$url" =~ ^[[:alnum:]]+://[^/]+/(.*)$ ]] || exit 3
printf '%s\n' "${BASH_REMATCH[1]}"
