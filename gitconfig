	;; msg...: Log variants that only cover the consecutive commit(s) from the passed -r|--revision REVISION / the first found revision whose messages match the specified text or /regexp/
	msgd = loggrange-command --with-log-args --with-range ... diff	; Diff from HEAD / the passed -r|--revision REVISION of consecutive commit(s) whose messages match the specified text or /regexp/.
	msgdsta = loggrange-command --with-log-args --with-range ... diff --shortstat
	msgdstat = loggrange-command --with-log-args --with-range ... diff --stat --compact-summary
	msgdstatbyeach = loggrange-command --with-log-args-except-last logstatbyeach
	msgdstabyeach = loggrange-command --with-log-args-except-last logstabyeach
	msgdl = loggrange-command --with-log-args --with-range ... dl
	msgdt = loggrange-command --with-log-args --with-range ... difftool
	msgdr = loggrange-command --with-log-args --with-range ... dr
	msgdrl = loggrange-command --with-log-args --with-range ... drl
	msgma = loggrange-command format-patch
	msglogg = loggrange-command --with-log-args-except-last logg
	msgloggmine = loggrange-command --with-log-args-except-last loggmine
	msgloggv = loggrange-command --with-log-args-except-last loggv
	msgloggvmine = loggrange-command --with-log-args-except-last loggvmine
	msglgg = loggrange-command --with-log-args-except-last lgg
	msglggmine = loggrange-command --with-log-args-except-last lggmine
	msglcg = loggrange-command --with-log-args-except-last lcg
	msglcgmine = loggrange-command --with-log-args-except-last lcgmine
	msglclg = loggrange-command --with-log-args-except-last lclg
	msglclgmine = loggrange-command --with-log-args-except-last lclgmine
	msgloggfiles = loggrange-command --with-log-args-except-last loggfiles
	msgloggversions = loggrange-command --with-log-args-except-last loggversions
	msgloggtags = loggrange-command --with-log-args-except-last loggtags
	msgchangedlog = loggrange-command --with-log-args-except-last changedlog
	msgchangedlogmine = loggrange-command --with-log-args-except-last changedlogmine
	msgchangedlogv = loggrange-command --with-log-args-except-last changedlogv
	msgchangedlogvmine = loggrange-command --with-log-args-except-last changedlogvmine
	msgchangedlg = loggrange-command --with-log-args-except-last changedlg
	msgchangedlgmine = loggrange-command --with-log-args-except-last changedlgmine
	msgchangedlc = loggrange-command --with-log-args-except-last changedlc
	msgchangedlcmine = loggrange-command --with-log-args-except-last changedlcmine
